[native "org.zenframework.z8.server.base.table.system.view.UsersView"]
class UsersView extends Users {
	protected UserForm form;
	form.users = this;
	form.flex = 1;

	colCount = 1;

	controls = { form };
	sortFields = { name };
	names = { name, lastName };
	quickFilters = { name, lastName };
}